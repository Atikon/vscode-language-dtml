{
    "name": "dtmllang",
    "version": "0.0.1",
    "engines": {
        "vscode":"^1.28.0"
    },
    "publisher": "me",
    "contributes": {
        "snippets": [{
            "language": "dtml",
            "path": "./snippets/snippets.json"
         }],
        "languages": [{
            "id": "dtml",
            "aliases": ["DTML", "dtml"],
            "extensions": [".dtl",".ztp", ".dtml"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "dtml",
            "scopeName": "text.dtml",
            "path": "./grammar/dtml.tmLanguage.json"
        }]
    }
}
  